{
  "name": "@w5s/fs",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Filesystem",
  "keywords": [
    "fp",
    "functional",
    "task",
    "fs"
  ],
  "homepage": "https://github.com/w5s/std/tree/master/packages/fs#readme",
  "bugs": {
    "url": "https://github.com/w5s/std/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/w5s/std.git",
    "directory": "packages/fs"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "type": "module",
  "exports": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib/**/!(*.spec).d.ts",
    "lib/**/!(*.spec).js.map",
    "lib/**/!(*.spec).js",
    "src/**/!(*.spec).ts",
    "!src/_test/**"
  ],
  "scripts": {
    "build": "npm run build:src",
    "build:src": "tsc",
    "clean": "npm run tsc -- --build --clean",
    "docs": "md-magic --path '**/*.md' --ignore='node_modules'",
    "format": "eslint . --ext=cjs,js,jsx,ts,tsx,json --fix",
    "lint": "eslint . --ext=cjs,js,jsx,ts,tsx,json",
    "test": "jest .",
    "tsc": "tsc",
    "validate": "npm run lint && npm run build && npm run test"
  },
  "dependencies": {
    "@types/node": "^17.0.21",
    "@w5s/core": "^1.0.0-alpha.0"
  },
  "peerDependencies": {
    "typescript": "^3.8.0 || ^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "typedocMain": "./src/index.ts"
}
