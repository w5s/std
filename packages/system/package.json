{
  "name": "@w5s/system",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "System module for w5s",
  "keywords": [
    "fp",
    "functional",
    "task",
    "fs"
  ],
  "homepage": "https://github.com/w5s/std/tree/master/packages/system#readme",
  "bugs": {
    "url": "https://github.com/w5s/std/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:w5s/std.git",
    "directory": "packages/system"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "type": "module",
  "exports": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib/**/!(*.spec).d.ts",
    "lib/**/!(*.spec).js.map",
    "lib/**/!(*.spec).js",
    "src/**/!(*.spec).ts",
    "!src/_test/**"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\" \":\"",
    "build:src": "tsc",
    "clean": "concurrently \"npm:clean:*\" \":\"",
    "clean:src": "rm -rf lib/*",
    "docs": "md-magic --path '**/*.md' --ignore='node_modules'",
    "format": "concurrently \"npm:format:*\" \":\"",
    "format:src": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "lint": "concurrently \"npm:lint:*\" \":\"",
    "lint:src": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "prepare": "concurrently \"npm:prepare:*\" \":\"",
    "prepublishOnly": "npm run clean;npm run build",
    "spellcheck": "cspell --no-progress '**'",
    "test": "concurrently \"npm:test:*\" ",
    "test:src": "jest",
    "tsc": "tsc",
    "validate": "npm run lint && npm run build && npm run test"
  },
  "dependencies": {
    "@types/node": "^17.0.21",
    "@w5s/core": "^1.0.0-alpha.0"
  },
  "devDependencies": {
    "@jest/globals": "29.4.1"
  },
  "peerDependencies": {
    "typescript": "^3.8.0 || ^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "typedocMain": "./src/index.ts"
}
