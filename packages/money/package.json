{
  "name": "@w5s/money",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Money package management",
  "keywords": [
    "fp",
    "functional",
    "task",
    "money"
  ],
  "homepage": "https://github.com/w5s/std/tree/master/packages/money#readme",
  "bugs": {
    "url": "https://github.com/w5s/std/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:w5s/std.git",
    "directory": "packages/money"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./lib/*": "./lib/*"
  },
  "typings": "./lib/index.d.ts",
  "files": [
    "lib/**/!(*.spec).d.ts",
    "lib/**/!(*.spec).js.map",
    "lib/**/!(*.spec).js",
    "src/**/!(*.spec).ts",
    "!example/**/*",
    "!*.cjs"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\" ",
    "build:src": "node scripts/buildCurrencies.js  && tsc",
    "clean": "concurrently \"npm:clean:*\" \":\"",
    "docs": "md-magic --path '**/*.md' --ignore='node_modules'",
    "format": "concurrently \"npm:format:*\" \":\"",
    "format:src": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json",
    "lint": "concurrently \"npm:lint:*\" \":\"",
    "lint:src": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json",
    "prepare": "concurrently \"npm:prepare:*\" \":\"",
    "spellcheck": "cspell --no-progress '**'",
    "test": "jest .",
    "tsc": "tsc",
    "validate": "npm run lint && npm run build && npm run test"
  },
  "dependencies": {
    "@w5s/core": "^1.0.0-alpha.0"
  },
  "devDependencies": {
    "@jest/globals": "29.0.2",
    "currencies.json": "1.0.2"
  },
  "peerDependencies": {
    "typescript": "^3.8.0 || ^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts"
  }
}
