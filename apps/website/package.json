{
  "name": "@w5s/website",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:w5s/std.git",
    "directory": "apps/website"
  },
  "license": "MIT",
  "scripts": {
    "build": "npx concurrently \"npm:build:*\" \":\"",
    "build:docusaurus": "docusaurus build",
    "clean": "npx concurrently \"npm:clean:*\" \":\"",
    "clean:docusaurus": "docusaurus clear",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "format": "npx concurrently \"npm:format:*\" \":\"",
    "format:src": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "lint": "npx concurrently \"npm:lint:*\" \":\"",
    "lint:src": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "prepare": "npx concurrently \"npm:prepare:*\" \":\"",
    "serve": "docusaurus serve",
    "spellcheck": "cspell --no-progress '**'",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "test": "npx concurrently \"npm:test:*\" \":\"",
    "typecheck": "tsc",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "3.1.1",
    "@docusaurus/module-type-aliases": "3.1.1",
    "@docusaurus/preset-classic": "3.1.1",
    "@mdx-js/react": "3.0.1",
    "clsx": "2.1.0",
    "docusaurus-plugin-typedoc-api": "4.2.0",
    "prism-react-renderer": "2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.1.1",
    "@docusaurus/tsconfig": "3.1.1",
    "@types/node": "20.11.30",
    "typescript": "5.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
